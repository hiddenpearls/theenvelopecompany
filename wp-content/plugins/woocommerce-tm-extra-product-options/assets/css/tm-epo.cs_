.tm-preloader-img:after{
    position:absolute;
    width:30px;
    height:30px;
    border-top:10px solid #fff;
    border-bottom:10px solid #fff;
    border-left:10px solid transparent;
    border-right:10px solid transparent;
    border-radius:50px;
    content:'';

